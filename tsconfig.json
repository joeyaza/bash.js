{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "lib": [
            "es6"
        ],
        "outDir": "dist",
        "declaration": true,
        "removeComments": false,
        "stripInternal": true,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true
    }
}